{
    "name": "dynamic/silverstripe-google-calendar",
    "description": "A Silverstripe module for displaying Google calendars.",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "calendar"
    ],
    "require": {
        "silverstripe/cms": "^5",
        "symbiote/silverstripe-gridfieldextensions": "^4"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\GoogleCalendar\\": "src/",
            "Dynamic\\GoogleCalendar\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "thirdparty/fullcalendar/lib",
            "client/js"
        ],
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
